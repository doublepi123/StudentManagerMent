<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lcy.studentmanagerment.ui.MainWindowControl">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="学生">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#AddStudent" text="添加" />
            <MenuItem fx:id="iexit" mnemonicParsing="false" onAction="#UpdateStudent" text="查询并修改" />
                  <MenuItem mnemonicParsing="false" onAction="#DeleteStudent" text="删除" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="课程">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#addCourse" text="添加" />
                  <MenuItem mnemonicParsing="false" onAction="#UpdateCourse" text="查询并修改" />
                  <MenuItem mnemonicParsing="false" onAction="#DeleteCourse" text="删除" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="选课信息">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#addSC" text="添加" />
                  <MenuItem mnemonicParsing="false" onAction="#UpdateSC" text="查询并修改" />
                  <MenuItem mnemonicParsing="false" onAction="#DeleteSC" text="删除" />
               </items>
            </Menu>
        <Menu mnemonicParsing="false" text="关于">
          <items>
            <MenuItem fx:id="bAbout" mnemonicParsing="false" onAction="#showAbout" text="关于" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <SplitPane focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
