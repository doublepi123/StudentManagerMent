        <AnchorPane>
          <children>
            <Label fx:id="mWelcome" alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" prefWidth="-1.0" style="&#10;" text="Master" textAlignment="CENTER" wrapText="false">
              <font>
                <Font size="18.0" fx:id="x1" />
              </font>
              <textFill>
                <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
              </textFill>
            </Label>
                  <Button fx:id="bexit" layoutX="387.0" layoutY="197.0" mnemonicParsing="false" onAction="#exit" text="退出" />
                  <Button fx:id="bAddStudent" layoutX="151.0" layoutY="54.0" mnemonicParsing="false" onAction="#AddStudent" text="添加" />
                  <Label layoutX="14.0" layoutY="54.0" text="学生信息：">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Button fx:id="bUpdateStudent" layoutX="223.0" layoutY="54.0" mnemonicParsing="false" onAction="#UpdateStudent" text="查询并修改" />
                  <Button layoutX="328.0" layoutY="54.0" mnemonicParsing="false" onAction="#DeleteStudent" text="删除" />
                  <Label layoutX="14.0" layoutY="99.0" text="课程信息：">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label layoutX="14.0" layoutY="149.0" text="选课信息：">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Button layoutX="151.0" layoutY="99.0" mnemonicParsing="false" onAction="#addCourse" text="添加" />
                  <Button layoutX="151.0" layoutY="149.0" mnemonicParsing="false" onAction="#addSC" text="添加" />
                  <Button layoutX="223.0" layoutY="99.0" mnemonicParsing="false" onAction="#UpdateCourse" text="查询并修改" />
                  <Button layoutX="223.0" layoutY="149.0" mnemonicParsing="false" onAction="#UpdateSC" text="查询并修改" />
                  <Button layoutX="328.0" layoutY="99.0" mnemonicParsing="false" onAction="#DeleteCourse" text="删除" />
                  <Button layoutX="328.0" layoutY="149.0" mnemonicParsing="false" onAction="#DeleteSC" text="删除" />
          </children>
        </AnchorPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
